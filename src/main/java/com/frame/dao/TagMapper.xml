<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frame.dao.TagMapper">
    <insert id="createTag" useGeneratedKeys="true">
        INSERT INTO tag(tag_name, artwork_id) VALUES (#{1},#{0})
    </insert>
    <select id="getArtworkIdByTag" resultType="int">
        SELECT artwork_id from tag where tag_name=#{0} limit #{1},20
    </select>
</mapper>
